{
	"description": "hymie phpmvc framework",
	"name": "hymie/mvc",
	"type": "framework",
	"version": "v0.0.1",
	"license": "MIT",
	"authors": [
		{
			"name": "mhx",
			"email": "mahaixing@gmail.com",
			"role": "developer"
		}
	],
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://packagist.phpcomposer.com"
		}
	},
	"config": {
        "platform": {
            "php": "7.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
	"bin": [
		"hymie"
	],
	"autoload": {
        "classmap": [
            "src/"
		],
		"files": ["src/hymie/function.php"]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
	},
	"require": {
		"php": "^7.1",
		"doctrine/annotations": "^1.8",
		"monolog/monolog": "^1.25",
		"psr/cache": "^1.0",
		"psr/log": "^1.1",
		"psr/simple-cache": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.5",
		"symfony/var-dumper": "^3.4"
	}
}
